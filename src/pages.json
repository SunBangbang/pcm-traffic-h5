{
	"easycom": {
			"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
		},
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
		    "path" : "pages/login/login",
		    "style" :                                                                                    
		    {
		        "navigationStyle": "custom"
		    }
		    
		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "智慧交通管理平台",
				 "navigationStyle": "custom"
			}
		}
        ,{
            "path" : "pages/home/home",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "智慧交通管理平台",
                // "enablePullDownRefresh": "custom",
				 "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/report/report",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "违停上报",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/violationReport/violationReport",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "违停上报",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/reportRecord/reportRecord",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "上报记录",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/stopReport/stopReport",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "上报成功",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "智慧交通管理平台",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
